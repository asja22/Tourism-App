<center>
<div class="container">
<div class="center jumbotron">
<h1>List of All Users</h1>
<table class="table table-hover table-bordered" >
  <tr>
    <th>Agent Name &nbsp; &nbsp; </th>
	<th>E-Mail &nbsp; &nbsp; </th>
	<th>Date of Joining &nbsp; &nbsp; </th>
	<th> &nbsp; &nbsp; </th>	
  </tr>
<% @user.each do |user| %>
  <tr>
    <td><%= user.name%></td> 
	<td><%= user.email%></td> 
	<td><%= user.created_at.to_date%></td> 
	<td><%= link_to "View", user_path(user) %></td> 
  </tr>
<% end %>
</table>
</div>
</div>
	<p><%= link_to "Back", admin_home_path, class: "btn btn-primary" %></p>
	<p><%= link_to "Back to home", root_path, class: "btn btn-primary" %></p>
</center>